{
    "name": "yousefkadah/laravel-memory-profiler",
    "description": "A Laravel package for memory profiling Artisan commands without Xdebug",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yousef Kadah",
            "email": "yousefkadah@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "laravel/pint": "^1.24",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "YousefKadah\\LaravelMemoryProfiler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YousefKadah\\LaravelMemoryProfiler\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YousefKadah\\LaravelMemoryProfiler\\MemoryProfilerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
